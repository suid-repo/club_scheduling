                        <li>@I18N.WebApplication.Constitution_CodeOfConduct_Li4</li>
                        <li>@I18N.WebApplication.Constitution_CodeOfConduct_Li5</li>
                        <li>@I18N.WebApplication.Constitution_CodeOfConduct_Li6</li>
                        <li>@I18N.WebApplication.Constitution_CodeOfConduct_Li7</li>
